{
  "dependencies": {
    "gulp": "3.9.1",
    "typescript": "^2.9.2"
  },
  "description": "<%= description %>",
  "devDependencies": {
    "@evan.network/angular-gulp": "^1.0.2",
    "@evan.network/api-blockchain-core": "^1.0.2",
    "@evan.network/smart-contracts-core": "^1.0.2",
    "@evan.network/ui-angular-core": "^1.0.2",
    "@evan.network/ui-angular-libs": "^1.0.2",
    "@evan.network/ui-angular-sass": "^1.0.2",
    "@evan.network/ui-dapp-browser": "^1.0.2"
  },
  "keywords": [
    "javascript",
    "typescript",
    "API",
    "Angular 5",
    "Ionic",
    "DApp",
    "SASS"
  ],
  "license": "Apache-2.0",
  "name": "<%= projectName %>",
  "scripts": {
    "dapp-build": "gulp --cwd . --gulpfile node_modules/@evan.network/angular-gulp/dapp.js build --dapp",
    "dapp-sass": "gulp --cwd . --gulpfile node_modules/@evan.network/angular-gulp/dapp.js sass --dapp",
    "dapps-build": "gulp --cwd . --gulpfile node_modules/@evan.network/angular-gulp/dapps.js dapps-build",
    "dapps-serve": "gulp --cwd . --gulpfile node_modules/@evan.network/angular-gulp/dapps.js dapps-build --serve",
    "deploy": "gulp --gulpfile node_modules/@evan.network/ui-dapp-browser/gulpfile.js deploy --config",
    "serve": "gulp --gulpfile node_modules/@evan.network/ui-dapp-browser/gulp/serve.js serve"
  },
  "version": "1.0.0"
}